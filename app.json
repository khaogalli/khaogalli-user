{
  "expo": {
    "name": "KhaoGalli Customer",
    "slug": "KhaoGalli",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/download.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/download.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/download.jpeg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/download.jpeg"
    },
    "plugins": [
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }]
    ]
  }
}
